--------------------------------------------------------------------------------------------------------------
--Notification Function
local function Notification(Title, Text)
    game.StarterGui:SetCore("SendNotification", {
    Title = Title,
    Text = Text,
    Duration = 5,
    })
end
--------------------------------------------------------------------------------------------------------------
--SpellCheck

if not table.find(Classes, Class) then
 Notification("Notification", "Stand name typed incorrectly.")
    end
end
